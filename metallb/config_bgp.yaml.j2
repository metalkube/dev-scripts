---
apiVersion: v1
kind: ConfigMap
metadata:
  namespace: metallb-system
  name: config
data:
  config: |
    peers:
    - peer-address: {{ metallb_frr_address }}
      peer-asn: 64512
      my-asn: 64512
    address-pools:
    - name: dev-env-bgp
      protocol: bgp
      addresses:
      - 192.168.10.0/24
